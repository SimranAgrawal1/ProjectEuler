public class A2
{
	public static void main(String[] args)
  {
		int a=1,b=2,sum=2;
		System.out.print(a+" "+b);
		for(int i=1;i<=5;i++)
		{
			a=a+b;
			b=a+b;
			System.out.print(" "+a+" "+b);
			if(a%2==0)
				sum=sum+a;
			else if(b%2==0)
				sum=sum+b;
			
		}
		System.out.println("\nSum="+sum);

	}

}
