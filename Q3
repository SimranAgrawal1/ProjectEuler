public class LargestPrimeFactor {

	public static void main(String[] args) {
		long  a= 600851475143l;
		long fact=0;
		boolean bool=true;
		for(long i=2;i<a;i++)
		{
			if(a%i==0)
			{
				 for (int j = 2; j < i; j++)
				 {
			            if (i % j == 0)
			            	bool=false;
				 }
				 if(bool==true)
					 fact=i;
			}
			
		}
		System.out.println(fact);

	}

}
